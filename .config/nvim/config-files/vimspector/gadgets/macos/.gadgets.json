{
  "adapters": {
    "CodeLLDB": {
      "command": [
        "${gadgetDir}/CodeLLDB/adapter/codelldb",
        "--port",
        "${unusedLocalPort}"
      ],
      "configuration": {
        "args": [],
        "cargo": {},
        "cwd": "${workspaceRoot}",
        "env": {},
        "name": "lldb",
        "terminal": "integrated",
        "type": "lldb"
      },
      "name": "CodeLLDB",
      "port": "${unusedLocalPort}",
      "type": "CodeLLDB"
    },
    "debugpy": {
      "command": [
        "/usr/local/opt/python@3.9/bin/python3.9",
        "${gadgetDir}/debugpy/build/lib/debugpy/adapter"
      ],
      "configuration": {
        "python": "/usr/local/opt/python@3.9/bin/python3.9",
        "subProcess": false
      },
      "name": "debugpy"
    },
    "multi-session": {
      "host": "${host}",
      "port": "${port}"
    },
    "vscode-cpptools": {
      "attach": {
        "pidProperty": "processId",
        "pidSelect": "ask"
      },
      "command": [
        "${gadgetDir}/vscode-cpptools/debugAdapters/OpenDebugAD7"
      ],
      "configuration": {
        "args": [],
        "cwd": "${workspaceRoot}",
        "environment": [],
        "type": "cppdbg"
      },
      "name": "cppdbg"
    },
    "vscode-go": {
      "command": [
        "node",
        "${gadgetDir}/vscode-go/dist/debugAdapter.js"
      ],
      "configuration": {
        "cwd": "${workspaceRoot}"
      },
      "name": "delve"
    }
  }
}